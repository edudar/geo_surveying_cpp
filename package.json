{
  "name": "geo-surveying",
  "productName": "geo-surveying",
  "version": "1.0.0",
  "description": "An app that solves a few geo-surveying tasks",
  "main": "src/main.js",
  "scripts": {
    "clean": "node-gyp clean",
    "configure": "node-gyp configure -- -f xcode",
    "install": "node-gyp build --target=7.9.0 --arch=x64 --dist-url=https://atom.io/download/electron && node_modules/.bin/electron-rebuild",
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": "https://github.com/kgo87/geo_surveying_cpp",
  "keywords": [
    "Electron"
  ],
  "author": "Kate Gomozova",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^4.1.5",
    "electron-packager": "^10.1.0",
    "electron-prebuilt-compile": "1.7.9",
    "electron-rebuild": "^1.6.0",
    "electron-builder": "^19.47.1"
  },
  "dependencies": {
    "bindings": "^1.2.1",
    "bootstrap": "^4.0.0-beta.2",
    "electron-compile": "^6.4.2",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.2.1",
    "popper.js": "^1.12.9"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "geo_surveying"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "geosurveying"
      }
    }
  }
}
